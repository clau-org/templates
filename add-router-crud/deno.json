{
  "name": "users",
  "version": "0.0.4",
  "tasks": {
    "cache": "deno cache --reload src/main.ts",
    "dev": "deno run -A --watch src/main.ts",
    "test": "deno test -A",
    "test:dev": "deno test -A --watch",
    "preview": "deno run -A src/main.ts",
    "deploy:test": "deployctl deploy --project='api-<NAME>' --exclude='node_modules' src/main.ts ",
    "deploy:prod": "deployctl deploy --project='api-<NAME>' --prod --exclude='node_modules' src/main.ts "
  }
}
